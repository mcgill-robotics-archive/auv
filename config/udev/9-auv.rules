# FTDI USB Serial Devices
KERNEL=="ttyUSB*", ATTRS{serial}=="DN00YSJG", SYMLINK+="thrusters"
KERNEL=="ttyUSB*", ATTRS{serial}=="MR2GL00X", SYMLINK+="dvl"
KERNEL=="ttyUSB*", ATTRS{serial}=="3b1MQYAA", SYMLINK+="imu"

# AdaFruit Feather32u4
KERNEL=="ttyACM*", ATTRS{idProduct}=="800c", ATTRS{idVendor}=="239a", SYMLINK+="depth", MODE:="0666"
ATTRS{idProduct}=="000c", ATTRS{idVendor}=="239a", MODE:="0666"


<launch>
  <arg name="rviz_config" default="$(find auv_viz)/config/robot.rviz" />

  <include file="$(find auv_description)/launch/description.launch" />
  <node name="auv_rviz_sim" pkg="auv_viz" type="sim_auv.py"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rviz_config)"/>
</launch>

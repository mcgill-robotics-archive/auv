<launch>
    <node name="XimuPublisher" type="XimuPublisher" pkg="state_estimation" respawn="true" respawn_delay="5" />

	<node name="tf_broadcaster" type="tf_broadcaster" pkg="state_estimation" />

	<node name="target_broadcaster" type="target_broadcaster" pkg="state_estimation" />

	<node name="initialize_horizon" type="initialize_horizon" pkg="state_estimation">
        <param name="yaw_drift_tolerance" value="1" />  <!-- in rad/sec -->
    </node>

    <node name="depth" type="depth" pkg="state_estimation">
        <param name="offset" value="-92.6" />
        <param name="window" value="10" />
    </node>

    <node name="decomposer" type="decomposer.py" pkg="state_estimation">
    </node>
</launch>
